{
  "name": "websolut-desktop",
  "version": "0.6.2-alpha",
  "description": "An Electron application with Vue and TypeScript",
  "private": true,
  "license": "MIT",
  "main": "./out/main/index.js",
  "config": {},
  "type": "module",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "test": "vitest",
    "format": "prettier --write .",
    "lint": "eslint . --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "concurrently --kill-others --kill-signal SIGTERM \"electron-vite dev\" \"vite build --watch\"",
    "build:spa": "vite build",
    "build:prepare-local": "node scripts/prepare-local-deps.cjs",
    "build": "npm run typecheck && npm run build:prepare-local && npm run build:spa && electron-vite build",
    "build:win": "npm run build && electron-builder -c electron-builder-no-sign.yml --win",
    "build:mac": "npm run build && electron-builder -c electron-builder-no-sign.yml --mac",
    "build:linux": "npm run build && electron-builder -c electron-builder-no-sign.yml --linux",
    "postinstall": "echo \"Skipping 'electron-builder install-app-deps'\"",
    "install-app-deps": "electron-builder install-app-deps",
    "dist": "dotenv -e .env -- npm run build && dotenv -e .env -- electron-builder",
    "dist:mac": "dotenv -e .env -- npm run build && dotenv -e .env -- electron-builder --mac --universal",
    "dist:mac:arm64": "dotenv -e .env -- npm run build && dotenv -e .env -- electron-builder --mac --arm64",
    "dist:mac:x64": "dotenv -e .env -- npm run build && dotenv -e .env -- electron-builder --mac --x64",
    "dist:win": "dotenv -e .env -- npm run build && dotenv -e .env -- electron-builder --win",
    "dist:linux": "dotenv -e .env -- npm run build && dotenv -e .env -- electron-builder --linux",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@iconify/vue": "^5.0.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/vite": "^4.1.11",
    "@types/morphdom": "^2.3.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/tsconfig": "^0.7.0",
    "@wsolut/websolut-core": "file:../../packages/core",
    "autoprefixer": "^10.4.21",
    "axios": "^1.10.0",
    "call-bind-apply-helpers": "^1.0.2",
    "chokidar": "^4.0.3",
    "class-validator": "^0.14.2",
    "electron-updater": "^6.3.9",
    "morphdom": "^2.7.7",
    "postcss-selector-parser": "^6.1.2",
    "radix-vue": "^1.9.17",
    "reflect-metadata": "^0.2.2",
    "slugify": "^1.6.6",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^4.1.11",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.11",
    "vue-router": "^4.5.1",
    "vue-tsc": "^2.2.8",
    "vue3-spinners": "^1.3.1",
    "websolut-backend": "file:../backend"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/vite": "^4.1.11",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "dotenv-cli": "^10.0.0",
    "electron": "^35.1.5",
    "electron-builder": "^25.1.8",
    "electron-notarize": "^1.2.2",
    "electron-osx-sign": "^0.6.0",
    "electron-vite": "^3.1.0",
    "eslint": "^9.24.0",
    "eslint-plugin-vue": "^10.0.0",
    "husky": "^9.1.7",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "vite": "^6.2.6",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^3.2.4",
    "vue": "^3.5.13",
    "vue-eslint-parser": "^10.1.3",
    "vue-tsc": "^2.2.8",
    "xml-name-validator": "^4.0.0"
  }
}
