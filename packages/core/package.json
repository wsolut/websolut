{
  "name": "@wsolut/websolut-core",
  "version": "0.5.3-alpha",
  "description": "Utilitary classes to convert Figma Nodes to a JSON format closer to DOM.",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "contributors": [
    "Joao Goncalves",
    "Dahiya Kishan"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsx --env-file=.env scripts/dev.ts",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:debug": "vitest --inspect-brk --no-file-parallelism",
    "build": "npm run clean && tsup",
    "clean": "rimraf dist tmp",
    "lint": "eslint . --fix",
    "format": "prettier .",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "tsup": "^8.3.0",
    "@types/node": "^20.14.10",
    "@figma/rest-api-spec": "^0.33.0",
    "@types/js-beautify": "^1.14.3",
    "@types/mime-types": "^3.0.1",
    "@types/ejs": "^3.1.5",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "nock": "^14.0.4",
    "prettier": "^3.4.2",
    "prettier-eslint": "^16.4.2",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.33.0",
    "vitest": "^3.2.0"
  },
  "dependencies": {
    "csstype": "^3.1.3",
    "ejs": "^3.1.10",
    "image-size": "^2.0.2",
    "js-beautify": "^1.15.4",
    "mime-types": "^3.0.1",
    "slugify": "^1.6.6"
  }
}
